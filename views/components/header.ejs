<div class="header">

    <!-- Header: Beinhaltet Navigation, h1-Pagetitle, Logos und Sociallinks -->

    <div class="container">
        <div class="wrapper">

            <div class="left">
                <nav class="navigation">
                    <%- printHeaderMenuHere(); -%>
                </nav>
                <h1><%- pageTitle -%></h1>
                <div class="homelogo"></div>
            </div>

            <div class="right">
                <nav class="socials">
                    <a class="logo-anchor" href="<%- ROOT -%>"><div class="smalllogo"></div></a>
                    <div class="divider"></div>
                    <a class="anchor-button" target="_blank" href="<%-  getConfigValue("firstsociallink")  -%>"><%-  getConfigValue("firstsocialtext")  -%></a>
                    <a class="anchor-button" target="_blank" href="<%-  getConfigValue("secondsociallink")  -%>"><%-  getConfigValue("secondsocialtext")  -%></a>
                </nav>
            </div>

        </div>
    </div>

</div>